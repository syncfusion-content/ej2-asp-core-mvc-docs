Html.EJS().AccumulationChart("container").Series(series =>
            {
                series.DataSource(ViewBag.dataSource)
                      .XName("x")
                      .YName("y")
                      .InnerRadius("65%").Add();
            })
            .CenterLabel(cl => cl.Text("Mobile<br>Browsers<br>Statistics")).LegendSettings(ls => ls.Visible(false)).Render()

