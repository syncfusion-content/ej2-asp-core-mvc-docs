@Html.EJS().Chart("container").Series(series =>
   {
   series.Type(Syncfusion.EJ2.Charts.ChartSeriesType.Hilo).  
   XName("x").
   High("high").
   Low("low").
   DataSource(ViewBag.dataSource).
   Opacity(0.5).
   Fill("blue").
   Add();

   }).
   PrimaryXAxis(px => px.ValueType(Syncfusion.EJ2.Charts.ValueType.Category)).
   Render()
