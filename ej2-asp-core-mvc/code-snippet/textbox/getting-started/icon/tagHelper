<div class="control-section">
      <ejs-textbox id="default" placeholder="Enter Date" created="createHanlder"></ejs-textbox>   
</div>

<style>
    .e-icons.e-input-group-icon.e-input-popup-date {
        font-size:16px;
    }

    .e-icons.e-input-group-icon.e-input-popup-date:before {
        content: "\e901";
    }

</style>

<script>
    
    function createHanlder(){
    
       var TextBoxInstance = document.getElementById('default').ej2_instances[0];

        TextBoxInstance.addIcon('append', 'e-icons e-input-group-icon e-input-popup-date');
    }
</script>