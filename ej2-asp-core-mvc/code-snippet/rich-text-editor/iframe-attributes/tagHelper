<ejs-richtexteditor id="iframe" created="created">
    <e-richtexteditor-iframesettings enable="true"></e-richtexteditor-iframesettings>
</ejs-richtexteditor>
<script>
    function created() {
        this.setProperties({
            iframeSettings: {
                attributes: {
                    readonly: 'readonly'
                }
            }
        }, false);
    }
</script>