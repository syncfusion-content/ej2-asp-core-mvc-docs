</ejs-circularchart3d>

<div>
    <ejs-button id="export" content="Export" isPrimary="true"></ejs-button>
</div>
<script>
    document.getElementById('export').onclick = () => {
        var circularchart = document.getElementById('container').ej2_instances[0];
        circularchart.export('PNG', 'result');
    };
</script>