<script>
    function created() {
        //Add Data Validation to range.
        this.addDataValidation({ type: 'Custom', value1: ' =AND(B1>10, B1<100)' }, 'E1:E5');
        //Highlight Invalid Data.
        this.addInvalidHighlight('E1:E5');
    }
</script>