@Html.EJS().Spreadsheet("spreadsheet").OpenUrl("Home/Open").SaveUrl("Home/Save").Created("created").Render()

<script>
    var base64String = "data:application/vnd.openxmlformats-officedocument.spreadsheetml.sheet;base64,";
    function created() {
        var spreadsheetObj = ej.base.getComponent(document.getElementById('spreadsheet'), 'spreadsheet');
        fetch(base64String)
            // To obtain blob data from base64 string.
            .then((response) => response.blob())
            .then((fileBlob) => {
                // To convert obtained blob data as a file.
                var file = new File([fileBlob], 'Sample.xlsx');
                spreadsheetObj.open({ file: file });
            });
    }

</script>