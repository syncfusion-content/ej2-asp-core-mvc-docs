@using Syncfusion.EJ2;
@Html.EJS().Maps("maps").Layers(l =>
   {
       l.UrlTemplate("https://tile.googleapis.com/v1/2dtiles/level/tileX/tileY?session=${sessionToken}&key=${apiKey}").Add();
       l.ShapeSettings(s => s.Fill("blue")).ShapeData(ViewBag.africaMapShape).Type(Syncfusion.EJ2.Maps.Type.SubLayer).Add();
   }).Render()

<style>
    #maps {
        display: block;
    }
</style>