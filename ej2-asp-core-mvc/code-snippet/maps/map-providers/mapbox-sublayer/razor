@using Syncfusion.EJ2;
@Html.EJS().Maps("maps").Layers(l =>
   {
       l.UrlTemplate("https://api.mapbox.com/v4/mapbox.satellite/level/tileX/tileY.jpg?access_token=Your_Key").Add();
       l.ShapeSettings(s => s.Fill("blue")).ShapeData(ViewBag.africaMapShape).Type(Syncfusion.EJ2.Maps.Type.SubLayer).Add();
   }).Render()

<style>
    #maps {
        display: block;
    }
</style>