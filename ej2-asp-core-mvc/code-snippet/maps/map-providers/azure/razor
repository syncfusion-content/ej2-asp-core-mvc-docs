@using Syncfusion.EJ2;
@Html.EJS().Maps("maps").Layers(l=> {
    l.UrlTemplate("https://atlas.microsoft.com/map/tile?api-version=2022-08-01&tilesetId=microsoft.base.road&zoom=level&x=tileX&y=tileY&subscription-key=Your_Key").Add();
}).Render()

<style>
    #maps {
        display: block;
    }
</style>
