@model List<object>

@Html.EJS().TreeView("listdata").Fields(field=>
   field.Id("id").ParentID("pid").Selected("selected").
   Expanded("expanded").Text("name").HasChildren("hasChild").HtmlAttributes("htmlAttributes")
   .DataSource(Model)).Render()

<style>
.child-node {
  font-weight: bold;
}
</style>